{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "types": ["node"],
    "lib": ["ES2022"],
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}