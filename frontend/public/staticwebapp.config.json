{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.webp",
      "/*.ico",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.map"
    ]
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html"
    },
    "403": {
      "rewrite": "/index.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  }
}